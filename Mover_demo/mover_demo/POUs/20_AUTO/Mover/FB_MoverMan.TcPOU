VAR_INPUT
    Mover : REFERENCE TO FB_Mover;
END_VAR
VAR
    moverIndex : INT;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[moverIndex := THIS^.GetMoverIndex(Mover);

IF moverIndex >= THIS^.MOVER_NUMBER THEN
    GetNextMover REF= THIS^._MoverList[1];
