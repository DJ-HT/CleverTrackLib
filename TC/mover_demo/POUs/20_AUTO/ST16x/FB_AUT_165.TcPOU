    <Property Name="xMoverPathFree" Id="{d2f1528f-1ec9-4add-a42a-10eacc2d179f}">
      <Declaration><![CDATA[PROPERTY xMoverPathFree : BOOL]]></Declaration>
      <Get Name="Get" Id="{e50fd281-97d7-4f19-a6f7-415ba3c3d008}">
