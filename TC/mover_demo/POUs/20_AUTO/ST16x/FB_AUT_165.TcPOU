    <Property Name="xDone" Id="{5a8c145a-9f48-43cc-a903-58bdaf95bdb8}">
      <Declaration><![CDATA[PROPERTY xDone : BOOL]]></Declaration>
      <Get Name="Get" Id="{e4987784-a0f7-4081-91ff-f8627ee5ef47}">
